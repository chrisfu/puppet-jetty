JETTY_HOME="<%= @root %>/jetty"
JETTY_BASE="<%= @base %>"
JETTY_USER="<%= @user %>"
JETTY_SHELL="/bin/sh"
<% if @jetty_arguments %>
JETTY_ARGS="<%= @jetty_arguments %>"
<% end %>
TMPDIR="<%= @root %>/jetty/tmp"
<% if @java %>
JAVA="<%= @java %>"
<% end %>
<% if @java_options %>
JAVA_OPTIONS="<%= @java_options %>"
<% end %>

