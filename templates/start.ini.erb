<% if @configuration.keys.length > 0 %>
<%-
_modules = @configuration.fetch('modules', [])
_options = @configuration.fetch('options', {})
%>
--module=home-base-warning
<% _modules.each do |m| %>
--module=<%= m %>
<% end %>

<% _options.each do |o| %>
<% _options.fetch(o, {}).each do |k,v| %>
<%= k %>=<%= v %>
<% end %>
<% end %>

