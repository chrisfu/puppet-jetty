<% if @configuration.keys.lenght > 0 %>
<% 
  _modules = @configuration.fetch('modules', {})
  _options = @configuration.fetch('options', {})
%>
--module=home-base-warning
<% _modules.each do |module| %>
--module=<%= module %>
<% _options.fetch(module, {}).each do |k,v| %>
<%= k %>=<%= v %>
<% end %>
<% end %>
<% end %>
