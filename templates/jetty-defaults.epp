JETTY_HOME="<%= $::jetty::root %>/jetty"
JETTY_BASE="<%= $::jetty::base %>"
JETTY_USER="<%= $::jetty::user %>"
JETTY_SHELL="/bin/sh"
<% if $::jetty::arguments { -%>
JETTY_ARGS="<%= $::jetty::jetty_arguments %>"
<% } -%>
TMPDIR="<%= $::jetty::root %>/jetty/tmp"
<% if $::jetty::java { -%>
JAVA="<%= $::jetty::java %>"
<% } -%>
<% if $::jetty::java_options { -%>
JAVA_OPTIONS="<%= $::jetty::java_options %>"
<% } -%>
